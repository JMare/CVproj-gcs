    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="edu.monash.jmare.cvproj_gcs.Settings"
        tools:showIn="@layout/app_bar_settings">

            <TextView
                android:id="@+id/text_greyThreshMin"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyThreshMin"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyThreshMin"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:padding="5dp"
                android:text="000"
                android:layout_toRightOf="@id/text_greyThreshMin"
                android:inputType="numberDecimal"
                android:maxLength="3"
                android:layout_alignParentTop="true"/>

            <TextView
                android:id="@+id/text_greyThreshMax"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyThreshMax"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/text_greyThreshMin"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyThreshMax"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:padding="5dp"
                android:text="000"
                android:layout_toRightOf="@id/text_greyThreshMax"
                android:maxLength="3"
                android:layout_below="@id/edit_greyThreshMin"
                android:inputType="numberDecimal" />

            <TextView
                android:id="@+id/text_greyErodePix"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyErodePix"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/text_greyThreshMax"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyErodePix"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:maxLength="3"
                android:padding="5dp"
                android:text="000"
                android:layout_toRightOf="@id/text_greyErodePix"
                android:layout_below="@id/edit_greyThreshMax"
                android:inputType="numberDecimal" />

            <TextView
                android:id="@+id/text_greyDilatePix"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyDilatePix"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/text_greyErodePix"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyDilatePix"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:maxLength="3"
                android:padding="5dp"
                android:text="000"
                android:layout_toRightOf="@id/text_greyDilatePix"
                android:layout_below="@id/edit_greyErodePix"
                android:inputType="numberDecimal" />

            <TextView
                android:id="@+id/text_greyErodeIter"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyErodeIter"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/text_greyDilatePix"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyErodeIter"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:padding="5dp"
                android:maxLength="3"
                android:text="000"
                android:layout_toRightOf="@id/text_greyErodeIter"
                android:layout_below="@id/edit_greyDilatePix"
                android:inputType="numberDecimal" />

            <TextView
                android:id="@+id/text_greyDilateIter"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:text="greyDilateIter"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/text_greyErodeIter"
                android:padding="5dp"
                android:paddingEnd="50dp"
                android:textSize="20dp"/>

            <EditText
                android:id="@+id/edit_greyDilateIter"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:padding="5dp"
                android:maxLength="3"
                android:text="000"
                android:layout_toRightOf="@id/text_greyDilateIter"
                android:layout_below="@id/edit_greyErodeIter"
                android:inputType="numberDecimal" />


    </RelativeLayout>
